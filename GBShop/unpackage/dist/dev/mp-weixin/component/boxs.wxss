
.box{
	line-height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center; /* 水平居中 */
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center; /* 垂直居中 */
}
